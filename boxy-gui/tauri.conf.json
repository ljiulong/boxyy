{
  "productName": "Boxy",
  "version": "0.6.2",
  "identifier": "com.boxy",
  "build": {
    "devUrl": "http://localhost:5173",
    "frontendDist": "../gui-frontend/dist"
  },
  "app": {
    "windows": [
      {
        "title": "Boxy",
        "width": 1320,
        "height": 860,
        "resizable": true,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "主窗口启用核心事件能力",
          "windows": [
            "*"
          ],
          "permissions": [
            "core:default",
            "core:event:default",
            "shell:allow-open",
            "updater:default"
          ]
        }
      ]
    }
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/ljiulong/boxyy/releases/latest/download/latest.json"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEE0OTJDRDE3QjNCQUJCNTcKUldSWHU3cXpGODJTcERMbFRqMGY1V3ZudTFlNmZWRUxVOXpwTENncE5MRUVwa29XVlJtRmJVWE8K"
    }
  },
  "bundle": {
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all",
    "createUpdaterArtifacts": "v1Compatible"
  }
}
